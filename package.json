{
  "name": "bebras-monitor",
  "version": "0.1.0",
  "description": "Monitor bebras-guard counters.",
  "homepage": "https://github.com/France-ioi/bebras-monitor",
  "authors": [
    "SÃ©bastien Carlier <s.carlier@epixode.fr>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/France-ioi/bebras-monitor"
  },
  "bugs": {
    "url": "https://github.com/France-ioi/bebras-monitor/issues"
  },
  "scripts": {
    "bundle-babel": "jspm bundle --minify babel build/babel-bundle.js",
    "bundle-deps": "jspm bundle --minify 'frontend/**/*' - '[frontend/**/*]' build/dependency-bundle.js",
    "build": "jspm build frontend/index.js build/index.js",
    "develop": "NODE_ENV=development babel-watch --use-polling backend/index.js",
    "start": "NODE_ENV=production LISTEN=8001 babel-node backend/index.js",
    "flow": "flow"
  },
  "jspm": {
    "main": "index.js",
    "configFiles": {
      "jspm": "frontend/jspm-0.17.config.js"
    },
    "dependencies": {
      "babel-helper-builder-react-jsx": "npm:babel-helper-builder-react-jsx@^6.18.0",
      "babel-plugin-syntax-jsx": "npm:babel-plugin-syntax-jsx@^6.18.0",
      "bootstrap-sass": "npm:bootstrap-sass@^3.3.7",
      "classnames": "npm:classnames@^2.2.5",
      "epic-component": "npm:epic-component@^0.3.1",
      "epic-linker": "npm:epic-linker@^1.0.5",
      "font-awesome": "npm:font-awesome@^4.7.0",
      "lodash": "npm:lodash@^4.16.6",
      "normalize.css": "github:necolas/normalize.css@^5.0.0",
      "plugin-sass": "npm:plugin-sass@^0.0.1-security",
      "react-bootstrap": "npm:react-bootstrap@^0.30.6",
      "react-redux": "npm:react-redux@^4.4.5",
      "scss": "github:mobilexag/plugin-sass@^0.5.0",
      "superagent": "npm:superagent@^2.3.0",
      "systemjs-plugin-babel": "npm:systemjs-plugin-babel@^0.0.17"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^6.0.0",
      "babel-runtime": "npm:babel-runtime@^6.0.0",
      "clean-css": "npm:clean-css@^3.4.20",
      "core-js": "npm:core-js@^1.1.4",
      "css": "github:systemjs/plugin-css@^0.1.32",
      "module": "npm:jspm-nodelibs-module@^0.2.0"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "domain": "npm:jspm-nodelibs-domain@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "graceful-fs": "npm:graceful-fs@^4.1.9",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "querystring": "npm:jspm-nodelibs-querystring@^0.2.0",
      "react": "npm:react@^15.4.0",
      "react-dom": "npm:react-dom@^15.3.2",
      "redux": "npm:redux@^3.6.0",
      "redux-saga": "npm:redux-saga@^0.12.1",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zlib": "npm:jspm-nodelibs-zlib@^0.2.0"
    },
    "overrides": {
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.3.2": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:font-awesome@4.7.0": {
        "main": "css/font-awesome.css!",
        "jspmNodeConversion": false,
        "registry": "jspm",
        "dependencies": {
          "css": "0.1"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.17.2": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.2": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:sass.js@0.9.13": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      }
    }
  },
  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-watch": "^2.0.3",
    "flow-bin": "^0.35.0",
    "jspm": "^0.17.0-beta.32",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "body-parser": "^1.15.0",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "fingertree": "epixode/fingertree.js#master",
    "functional-red-black-tree": "^1.0.1",
    "immutable": "^3.8.1",
    "pug": "^2.0.0-beta5",
    "redis": "^2.6.3",
    "redux": "^3.6.0",
    "redux-saga": "^0.13.0",
    "systemjs": "^0.19.41"
  }
}
